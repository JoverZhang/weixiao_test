<!--pages/index/noticeDetail.wxml-->
<scroll-view class="noticeDetail" scroll-y="true" scroll-with-animation="" enable-back-to-top="ture" scroll-top="10000000000000">
    <view class="item" hover-class="none" hover-stop-propagation="false">
        <view class="time" hover-class="none" hover-stop-propagation="false">{{noticeSummary.time}}</view>
        <view class="content" hover-class="none" hover-stop-propagation="false">
            <text>{{noticeSummary.noticeSummary}}</text>
        </view>
    </view>
</scroll-view>